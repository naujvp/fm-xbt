%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\newcommand{\conaeinit}[3]{
    \lhead{\includegraphics[scale=0.3]{figuras/conae/conae_logo.png} \textbf{\textsl{#1}} \hspace{0.5cm} \textbf{\textsl{#2}}\hspace{1.75cm}  }
    \chead{}
    \rhead{\textbf{\textsl{{#3}}}}
    \lfoot{}
    \cfoot{}
    \rfoot{\textbf{\textsl{\lang{Page}{P\'agina} \thepage~\lang{of}{de} \pageref{LastPage}}}}
    % titulo tabla de contenidos. Problemas con comando \lang...
        \ifthenelse{\boolean{es}}{\renewcommand{\contentsname}{TABLA DE CONTENIDOS}}{\renewcommand{\contentsname}{TABLE OF CONTENTS}}
    
    \@addtoreset{figure}{section}
    \@addtoreset{figure}{subsection}
    \@addtoreset{table}{section}
    \@addtoreset{table}{subsection}
    % esquema de numeracion de figuras
    \renewcommand{\thefigure}{\thesubsubsection.-\alph{figure}}
    % esquema de numeracion de tablas
    \renewcommand{\thetable}{\thesubsubsection.-\alph{table}}
}
